<!-- Modal -->
<div class="modal fade" id="new_solution_modal" tabindex="-1" role="dialog" aria-labelledby="new_algorithm_Label" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h4 class="modal-title" id="new_algorithm_Label">New Solution</h4>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <%= render partial: 'solutions/form' %>
      </div>
    </div>
  </div>
</div>
